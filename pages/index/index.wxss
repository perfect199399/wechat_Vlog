@import '../../style/weui.wxss';
@import '../../style/weuiFlex.wxss';
.page{
  background: #ffffff;
}
.slide-image{ 
  width: 100%;
  height: 100%;
}
.demo{
  width: 100%;
  text-align: center;
  line-height: 3;
}
.image{
  width: 92%;
  height: 130px;
  border-radius: 6px;
}
.map{
  display: flex;
  padding-top: 20px;
  align-items: center;
}
.openMap{
  flex: 1;
  width: 100%;
  padding-right: 10px;
  height: 150rpx;
}
.strategy{
  width: 90%;
  height: 100rpx;
  margin:0 auto;
  font-size: 18px;
  line-height:3rem;
  background-image: linear-gradient(to right, #eea2a2 0%, #bbc1bf 19%, #57c6e1 42%, #b49fda 79%, #7ac5d8 100%);
  border-radius: 6px;
  text-align: center;
  color: #ffffff;
}
